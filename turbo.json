{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:docs": {
      "dependsOn": ["docs#build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:web": {
      "dependsOn": ["web#build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
